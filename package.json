{
  "name": "gen3-dmg-calc",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "prepare": "simple-git-hooks",
    "dev": "vite",
    "preview": "vite preview",
    "prebuild": "yarn typecheck && yarn lint",
    "build": "vite build",
    "typecheck": "vue-tsc --noEmit",
    "format": "prettier -w .",
    "lint": "eslint ."
  },
  "simple-git-hooks": {
    "pre-commit": "yarn nano-staged"
  },
  "nano-staged": {
    "**/*.{cjs,js,ts,vue}": [
      "eslint",
      "prettier -w"
    ],
    "**/*.{css,html,json,md}": [
      "prettier -w"
    ]
  },
  "dependencies": {
    "vue": "^3.2.45",
    "zod": "^3.20.2"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.4",
    "@types/node": "^18.11.12",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.22.0",
    "eslint-config-foxkit": "^2.0.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-vue": "^9.3.0",
    "nano-staged": "^0.8.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "simple-git-hooks": "^2.8.1",
    "typescript": "~4.7.4",
    "vite": "^4.0.0",
    "vue-tsc": "^1.0.12"
  }
}
